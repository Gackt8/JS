using System;
	
	public class A
	{
		public float a;
		public float b;
		
		public A(float a, float b)
		{
			this.a = a;
			this.b = b;
		}
		
		public float c
		{
			get { return a / b; }
			set { c = value; }
		}
	}


	public class B : A {
		public float d;
		
		public float c2
		{
			get { return a + b + d; }
			set { c2 = value; }
		}
		
		public B(float a, float b, float d) :
		base(a, b)
		{
			this.d = d;
		}
		
		public static void Main(string[] args)
		{
			B b = new B(5, 9, 6);
			
			if( b.a > 0 )
			{
				Console.WriteLine(b.c);
				Console.WriteLine(b.c2);
			}
			else {
				Console.WriteLine("Ok");
			}
		}
	}
