using System;
	
	public class A
	{
		public int a;
		public int b;
		 public A(int a, int b) {
			this.a = a;
			this.b = b;
		}
		
		public float c
		{
			get { return a / b; }
			set { c = value; }
		}
	}


	public class B : A {
		public int d;
		public int[] m;
		
		public int c2
		{
			get { return a + b + d; }
			set { c2 = value; }
		}
	
		
		public B(int a, int b, int d) :
			base(b, d) {
				this.a = a;
				this.b = b;
				this.d = d;
				m = new int[a];
			for (int i = 0; i < m.Length; i++)
			{
				m[i] = c2 * i;
			}
		}
		
		public static void Main(string[] args)
		{
			B classB = new B(16, 2, 6);
			
			foreach(int n in classB.m)
		{
			Console.Write("{0,8:N}", n);
		}
			Console.WriteLine();
		}
	}
